	"author": "SebastianMC",
	"repository": "https://github.com/SebastianMC/obsidian-recently-modified-dv",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"obsidian": "^0.16.3",
		"obsidian-dataview": "^0.5.47",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
