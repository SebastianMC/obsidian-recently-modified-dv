{
	"name": "obsidian-recently-modified-dv",
	"version": "0.1.0",
	"description": "Recently modified notes (dataview-based) - plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
